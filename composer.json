{
    "name": "sigwin/ariadne",
    "description": "Find the common thread on all of your Glabyrinth projects",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "knplabs/github-api": "^3.9",
        "m4tthumphrey/php-gitlab-api": "^11.0",
        "nyholm/psr7": "^1.5",
        "symfony/console": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/options-resolver": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "sigwin/infra": "~1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Sigwin\\Ariadne\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    }
}
